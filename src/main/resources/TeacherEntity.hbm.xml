<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.TeacherEntity" table="teacher" schema="online_skill_system">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(120)" length="120"/>
        </property>
        <property name="telephone">
            <column name="telephone" sql-type="varchar(120)" length="120" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(120)" length="120" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>